#!/data/data/com.termux/files/usr/bin/bash

pkg install pwgen
Pkg install termux-api

su -c pm install -r -d de.apk
su -c pm install anon.apk
su -c pm install api.apk

yes | cp -fr Update ~/.shortcuts/
yes | cp -fr Alamat ~/.shortcuts/tasks/
yes | cp -fr Clear ~/.shortcuts/tasks/
yes | cp -fr JDID ~/.shortcuts/tasks/
yes | cp -fr Start ~/.shortcuts/tasks/

chmod -R 700 ~/.shortcuts/

echo "Update selesai"

