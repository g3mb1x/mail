#!/data/data/com.termux/files/usr/bin/bash

#updateAPK
su -c pm install -r -d olook.apk

yes | cp -fr -t ~/.shortcuts/tasks/ Alamat Alamat2 Alamat3 Clear JDID Start Start2
yes | cp -fr -t ~/.shortcuts/ GantiHP
chmod -R 700 ~/.shortcuts/

yes | cp -fr -t ~/ a f m m2 jl dikan

su -c rm -rf /sdcard/data/BixDE/*
cp import.eg /sdcard/data/BixDE/

echo "Update selesai"
